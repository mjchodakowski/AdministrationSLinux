        </th>
        <th>
            @Html.DisplayNameFor(model => model.RootKey)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ip)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Port)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NazwaUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RootKey)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
