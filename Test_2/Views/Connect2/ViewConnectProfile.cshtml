@model IEnumerable<Test_2.Models.ProfilPolaczenia>

@{
    ViewBag.Title = "ViewConnectProfile";
}

<h2>ViewConnectProfile</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ip)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Port)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NazwaUser)
        </th>
        <th>
